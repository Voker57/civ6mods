<?xml version="1.0" encoding="utf-8"?>
<GameData>
	
	<Units>
		<Update><Where UnitType="UNIT_ARTILLERY"/><Set><StrategicResource>RESOURCE_NITER</StrategicResource><PrereqTech>TECH_RIFLING</PrereqTech></Set></Update>
		<Update><Where UnitType="UNIT_INFANTRY"/><Set><StrategicResource/><PrereqTech>TECH_STEEL</PrereqTech></Set></Update>
		<Update><Where UnitType="UNIT_BATTLESHIP"/><Set><StrategicResource>RESOURCE_OIL</StrategicResource><RangedCombat>75</RangedCombat></Set></Update>
		<Update><Where UnitType="UNIT_SUBMARINE"/><Set><StrategicResource/></Set></Update>
