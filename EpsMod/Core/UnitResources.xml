		<Delete UnitType="UNIT_SUBMARINE"/>
	</Units_XP2>

	<Improvements>
		<Update><Where ImprovementType="IMPROVEMENT_OFFSHORE_OIL_RIG"/><Set><PrereqTech>TECH_REFINING</PrereqTech></Set></Update>
	</Improvements>
	
</GameData>